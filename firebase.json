{
  "hosting": {
    "public": "public",
    "redirects": [ {
      "source": "chortle.denhoff.ca/**",
      "destination": "/chortle/**",
      "type": 301
    } ],
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ],
    "headers": [ {
        "source": ".well-known/matrix/client",
        "headers": [ {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
        } ]
    },
    {
        "source": ".well-known/matrix/client|.well-known/matrix/server",
        "headers": [ {
            "key": "Content-Type",
            "value": "application/json"
        } ]
     } ]
  }
}
